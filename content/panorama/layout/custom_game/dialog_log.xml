<root>
  <styles>
  <include src="s2r://panorama/styles/dotastyles.vcss_c" />

  <include src="s2r://panorama/styles/hudstyles.vcss_c"/>
  <include src="s2r://panorama/styles/hud/hud_reborn.vcss_c" /> 
    
  <include src="s2r://panorama/styles/custom_game/dialog_log.vcss_c" />

  </styles>
  
  <scripts>
  <include src="s2r://panorama/scripts/custom_game/dialog_log.vjs_c" />
  </scripts>

  <Panel hittest="false" class="DialogLogRoot">
    <Panel hittest="false" class="TopLeftMenu">
			<Button id="DialogLogButton" class="TopLeftMenuButton" onactivate="ToggleDialogLog();" onmouseover="UIShowTextTooltip('Message Log')"  onmouseout="UIHideTextTooltip()" />
		</Panel>

    <Panel id="DialogLog" hittest="false">
      <Label id="DialogTitle" class="TitleFont" text="Message Log" hittest="false"/>
      <Panel id="DialogLineContainer" />
      <Button id="CloseButton" class="ButtonBevel" onactivate="Close()"><Label text="Close"/></Button>
    </Panel>
  </Panel>
</root>